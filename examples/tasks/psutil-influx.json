{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "1s"
    },
    "workflow": {
        "collect": {
            "metrics": {                
                "/psutil/load/load1": {},
                "/psutil/load/load5": {},
                "/psutil/load/load15": {},
                "/psutil/vm/available": {},
                "/psutil/vm/free": {},
                "/psutil/vm/used": {}
            },
            "config": {
                "/intel/dummy": {
                    "password": "secret",
                    "user": "root"
                }
            },
            "process": [
                {
                    "plugin_name": "passthru",
                    "plugin_version": 1,
                    "process": null,
                    "publish": [
                        {
                            "plugin_name": "influx",
                            "plugin_version": 1,
                            "config": {
                                "host": "INFLUXDB_IP",
                                "port": 8086,
                                "database": "pulse",
                                "user": "admin",
                                "password": "admin"                            
                            }
                        }
                    ],
                    "config": null
                }
            ],
            "publish": null
        }
    }
}
