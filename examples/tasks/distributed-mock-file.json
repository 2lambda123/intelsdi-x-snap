{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "1s"
    },
    "failure": 10,
    "workflow": {
        "collect": {
            "metrics": {
                "/intel/mock/foo": {},
                "/intel/mock/bar": {},
                "/intel/mock/*/baz": {}
            },
            "config": {
                "/intel/mock": {
                    "user": "root",
                    "password": "secret"
                }
            },
            "process": [
                {
                    "plugin_name": "passthru",                    
		    "target": "127.0.0.1:9999", 
		    "process": null,
                    "publish": [
                        {
                            "plugin_name": "file",                            
			    "target": "127.0.0.1:9992",
                            "config": {
                                "file": "/tmp/snap_published_mock_file.log"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
